    "multer": "^1.2.0",
    "mysql": "^2.11.1",
    "normalize.css": "4.2.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "0.12.7",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-dropzone": "^3.6.0",
    "react-helmet": "3.1.0",
    "react-rating": "^0.5.0",
    "react-redux": "^4.4.5",
    "react-router": "2.8.1",
